{
  "version": 2,
  "name": "wehategluten-production-live",
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "static/**/*", "use": "@vercel/static" },
    { "src": "content/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/article/(.*)", "dest": "/article.html" },
    { "src": "/category/(.*)", "dest": "/category.html" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "github": {
    "silent": true
  }
}
